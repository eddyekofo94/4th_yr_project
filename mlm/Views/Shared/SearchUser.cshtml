@model mlm.Models.SearchViewModel.SearchUserViewModel
<section>
    <table class="table">
        <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Users[0].Email)
            </th>
            <th></th>
        </tr>
        </thead>
        <tbody>
        @foreach (var item in Model.Users)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>

                <td>
                    Add as friend
                </td>
            </tr>
        }
        </tbody>
    </table>
</section>